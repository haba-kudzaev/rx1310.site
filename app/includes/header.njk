<header class="header__wrapper">
	<div class="container">
		<div class="row">
			<div class="col-12">
				<div class="header">

					<img
						src="{{ header.image.url }}"
						alt="{{ header.image.alt }}"
						title="{{ header.image.alt }}"
						class="header__image">
						
					<h1 class="header__title">{{ header.title }}</h1>
					<p class="header__subtitle">{{ header.subtitle }}</p>

				</div>
			</div>
		</div>
	</div>
</header>